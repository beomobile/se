{
    "background": {
        "persistent": false,
        "scripts": [ "jquery.js", "eventPage.js" ]
    },
    "browser_action": {
        "default_icon": {
            "19": "img/icon.png",
            "38": "img/icon.png"
        }
    },
    "options_page": "options.html",
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.facebook.com; object-src 'self'",
    "icons": {
        "128": "img/icon.png",
        "16": "img/icon.png",
        "48": "img/icon.png"
    },
    "description": "Convert Facebook UID to Email",
    "homepage_url": "https://salekit.xyz/",
    "manifest_version": 2,
    "name": "Facebook UID to Email",
    "short_name": "UIDtoEmail",
    "permissions": [ "tabs", "https://*.facebook.com/*"],
    "version": "2.0.1.5"
}
